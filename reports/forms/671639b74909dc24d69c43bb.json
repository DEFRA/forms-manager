        "components": [
          {
            "name": "TzgpkF",
            "title": "Html",
            "type": "Html",
            "content": "<p class=\"govuk-body\">\nYour dog must be at least 18 months old before you can apply to withdraw a Certificate of Exemption.\n<br><br>\nYou can apply using this form when your dog reaches 18 months old.\n</p>",
            "options": {},
            "schema": {}
          }

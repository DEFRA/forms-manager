      {
        "path": "/grid-reference-of-the-site",
        "title": "Grid reference of the site",
        "components": [
          {
            "name": "pqdBNj",
            "title": "Html",
            "type": "Html",
            "content": "<p class=\"govuk-body\">To find a grid reference:\n</p>\n<ol class=\"govuk-list govuk-list--number\">\n  <li> Open the free <a href=\"https://explore.osmaps.com/\" class=\"govuk-link\" rel=\"noreferrer noopener\" target=\"_blank\">Ordnance Survey Maps (opens in new tab)</a>.\n  </li>\n  <li>Search using the site location or postcode.</li>\n  <li>Right-click or tap on the centre of the site on the map.</li>\n  <li>Copy the grid reference from the pop-up box.</li>\n</ol>",
            "options": {},
            "schema": {}
          },
          {
            "name": "eZgceC",
            "title": "What's the grid reference for the centre of the site?",
            "type": "TextField",
            "hint": "The grid reference should have 2 letters and 10 numbers, in the format SY 98463 56728.",
            "options": {
              "customValidationMessage": "Grid reference for the centre of the site"
            },
            "schema": {
              "regex": "^((([sS]|[nN])[a-hA-Hj-zJ-Z])|(([tT]|[oO])[abfglmqrvwABFGLMQRVW])|([hH][l-zL-Z])|([jJ][lmqrvwLMQRVW]))\\s?([0-9]{5})\\s?([0-9]{5})$",
              "max": 14,
              "min": 12
            }
          }
        ],
        "next": [
          {
            "path": "/whats-the-maximum-distance-from-the-centre-of-the-site-to-the-site-boundary"
          }
        ]
      },

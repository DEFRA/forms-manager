{
  "name": "@defra/forms-manager",
  "version": "0.0.0",
  "private": true,
  "description": "Defra forms manager",
  "license": "OGL-UK-3.0",
  "author": "Defra DDTS",
  "type": "module",
  "main": ".server/index.js",
  "scripts": {
    "build": "npm run build:server",
    "build:server": "babel --delete-dir-on-start --ignore \"**/*.test.js\" --ignore \"src/__fixtures__\" --copy-files --no-copy-ignored --keep-file-extension --out-dir ./.server ./src",
    "docker:dev": "NODE_ENV=development npm run server:watch",
    "dev": "npm run server:watch",
    "dev:debug": "npm run server:debug",
    "format": "prettier --write \"**/*.{cjs,js,json,md,mjs}\"",
    "format:check": "prettier --check \"**/*.{cjs,js,json,md,mjs}\"",
    "postinstall": "bash install_model.sh && npm run setup:husky",
    "lint": "npm run lint:js",
    "lint:fix": "eslint . --fix",
    "lint:js": "eslint .",
    "lint:types": "tsc --noEmit",
    "postversion": "git add package.json package-lock.json && git commit -m $npm_package_version",
    "test": "jest --coverage --verbose",
    "test:watch": "jest --watch",
    "server:watch": "nodemon --exec babel-node --enable-source-maps ./src",
    "server:debug": "nodemon --exec babel-node --enable-source-maps --inspect ./src",
    "prestart": "npm run build",
    "start": "NODE_ENV=production node --enable-source-maps ./.server",
    "setup:husky": "node -e \"try { (await import('husky')).default() } catch (e) { if (e.code !== 'ERR_MODULE_NOT_FOUND') throw e }\" --input-type module"
  },
  "dependencies": {
    "@babel/runtime": "7.23.9",
    "@elastic/ecs-pino-format": "1.5.0",
    "@hapi/boom": "10.0.1",
    "@hapi/hapi": "21.3.3",
    "aws4": "1.12.0",
    "babel-plugin-module-resolver": "5.0.0",
    "convict": "6.2.4",
    "hapi-pino": "12.1.0",
    "https-proxy-agent": "7.0.2",
    "joi": "17.12.1",
    "lodash": "4.17.21",
    "mongodb": "6.3.0",
    "node-fetch": "2.7.0",
    "pino": "8.18.0",
    "undici": "6.6.2"
  },
  "devDependencies": {
    "@babel/cli": "7.23.9",
    "@babel/core": "7.23.9",
    "@babel/node": "7.23.9",
    "@babel/preset-env": "7.23.9",
    "@typescript-eslint/eslint-plugin": "7.2.0",
    "@typescript-eslint/parser": "7.2.0",
    "babel-jest": "29.7.0",
    "babel-loader": "9.1.3",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-standard": "17.1.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsdoc": "48.2.1",
    "eslint-plugin-n": "16.6.2",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-promise": "6.1.1",
    "husky": "9.0.10",
    "jest": "29.7.0",
    "jsdoc": "4.0.2",
    "lint-staged": "15.2.2",
    "nodemon": "3.0.3",
    "npm-run-all": "4.1.5",
    "pino-pretty": "10.3.1",
    "prettier": "3.2.5",
    "resolve-url-loader": "5.0.0",
    "typescript": "5.4.2"
  },
  "optionalDependencies": {
    "@types/convict": "6.1.6",
    "@types/jest": "29.5.12",
    "@types/node": "20.11.26"
  },
  "engines": {
    "node": "^20.9.0",
    "npm": "^10.1.0"
  }
}
